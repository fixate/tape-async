{
  "name": "tape-async",
  "description": "",
  "repository": "https://github.com/parro-it/tape-async.git",
  "bugs": {
    "url": "https://github.com/parro-it/tape-async/issues"
  },
  "version": "1.0.2",
  "main": "src/index.js",
  "engines": {
    "node": ">=4.1"
  },
  "dependencies": {
    "co": "^4.6.0",
    "tape": "^4.2.0"
  },
  "author": "parro-it",
  "scripts": {
    "cover": "rm -rf coverage && istanbul cover test/index.js && codeclimate-test-reporter < coverage/lcov.info",
    "debug": "hihat --quit --node --no-electron-builtins test -- -p tap-dev-tool",
    "watch": "nodemon test | faucet",
    "test": "node test | faucet",
    "lint": "eslint src/*.js test/*.js",
    "check": "npm run lint && npm run test",
    "prepublish": "npm run check"
  },
  "eslintConfig": {
    "extends": "eslint-config-airbnb",
    "plugins": [
      "babel"
    ],
    "rules": {
      "func-names": 0,
      "comma-dangle": 0,
      "no-undef": 1,
      "strict": [
        0,
        "global"
      ]
    }
  },
  "babel": {
    "stage": 4
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "codeclimate-test-reporter": "^0.1.1",
    "electron-prebuilt": "^0.33.1",
    "eslint": "^1.5.0",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.4.2",
    "hihat": "^2.4.3",
    "tap-dev-tool": "^1.3.0",
    "istanbul": "^0.3.20",
    "nodemon": "^1.7.0",
    "faucet": "0.0.1"
  },
  "license": "MIT"
}
